                    "",
                    "                }",
                    "            });",
                    "",
                    "",
                    "        }",
                    "        if (this.action=='EDIT'){",
                    "            f_crud.load_store('Labores_insumos','id_labores=' + id);",
                    "            f_crud.load_store('Labores_personal','id_labores=' + id);",
