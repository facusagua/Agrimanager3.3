                    "            record.set('propia','P');",
                    "",
                    "            this.down('#cod_lote_actividad').setValue(codigo);",
                    "            this.down('#cantidad').setValue(form_panel.superficie);",
                    "",
                    "            // Genero el id de cabecera",
                    "",
                    "            f_crud.secuencia(function(rtn){",
                    "                if (rtn !== -1){",
